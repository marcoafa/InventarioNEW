<body>
    
   
   
        <div class="col-md-12" style="background-color:white;">

            
            <img src="~/Content/EstilosPersonalizados/Imagenes/logo.jpg" />

        </div>
    

    @{if (Session["nombre"] == null)
    {


        <div class="wrapper">
            <form method="post" class="form-signin form-horizontal" action="~/home/Login">
                <h3 class="col-md-4 col-md-offset-4 form-signin-heading">Login</h3>
                <div class="form-group">

                    <div class="col-md-6 col-md-offset-3">
                        <input type="text" class="form-control logintext" name="username" placeholder="Usuario" style="height:auto;padding:10px;" />
                    </div>
                </div>
                <div class="form-group">
                    <div class="col-md-6 col-md-offset-3">
                        <input type="password" class="form-control loginpassw" name="password" placeholder="Contraseña" style="height:auto;padding:10px;" />
                    </div>
                </div>
                <div class="form-group">
                    <div class="col-md-4 col-md-offset-4">
                        <button class="btn btn-lg btn-primary btn-block" type="submit">Login</button>
                    </div>
                </div>
            </form>
        </div>
    }
    }
    

    <div class="row">
        @RenderBody()
    </div>

    <script src="~/Scripts/jquery-2.1.4.min.js"></script>
