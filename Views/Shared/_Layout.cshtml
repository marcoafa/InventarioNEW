    <script src="~/Scripts/Inventario/Editar.js"></script>
    <script src="~/Scripts/fileinput.min.js"></script>

    <script src="~/Scripts/pdfobject.min.js"></script>
    <script src="~/Scripts/Inventario/VerFactura.js"></script>
